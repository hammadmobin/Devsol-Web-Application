		    <div class="form-head">
			 <h3>Login</h3>
			</div><!-- /.form-head -->
            <div class="form-body">
			
			 <div class="form-row">
			  <div class="form-controls">
			   <input name="username" placeholder="Username" class="field" type="text">
			  </div><!-- /.form-controls -->
			 </div><!-- /.form-row -->

			 <div class="form-row">
			  <div class="form-controls">
			   <input name="password" placeholder="Password" class="field" type="password">
			  </div><!-- /.form-controls -->
			 </div><!-- /.form-row -->
		    </div><!-- /.form-body -->

			<div class="form-foot">
			 <div class="form-actions">					
			  <input value="Login" class="form-btn" type="submit">
			 </div><!-- /.form-actions -->
             <div class="form-head">
			  <a href="#" class="more-link">Forgot Password?</a>
			 </div>
			</div><!-- /.form-foot -->
		   </form>
		   
		  </div><!-- /.form-sign -->
	     </div><!-- /.col-lg-6 -->
